#include <stdio.h>

int main()
{
    int num1, num2 ,res;
    char op;
    
    num1 = 90;
    num2 = 33;
    op = '+';
    
    switch (op)
    {
        case '+' :
            res = num1 + num2;
            printf("\n Sum is : %d", res);
            break;
        case '-' :
            res = num1 - num2;
            printf("\n Subtracted is : %d", res);
            break;
        case '/' :
            res = num1 - num2;
            printf("\n Divided is %d", res);
            break;
        case '*':
            res = num1 * num2;
            printf("\n Multiple is %d", res);
            break;
        default :
            printf("\n Invalid");
            break;
    }
    return 0;
    
}
