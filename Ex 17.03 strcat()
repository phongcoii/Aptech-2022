#include <stdio.h>
#include <string.h>

void main()
{
    char firstname[15];
    char lastname [15];
    
    printf("Enter your 1st name: ");
    scanf("%s", firstname);
    
    printf("Enter your last name: ");
    scanf("%s", lastname);
    
    strcat(firstname, lastname);
    printf("%s", firstname);
}
