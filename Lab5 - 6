#include <stdio.h>

void main()
{
    int n;
    int a;
    int multi;
    char ch;
    
    printf(" Enter number : ");
    scanf("%d", &n);
    
    for ( a = 1; a <= 9; a++)
    {
    {
        multi = n * a;
        printf("%d * %d = %d\n", n, a, multi);
    }

        printf(" Continue ?? \n");
        fflush(stdin);
        scanf("%c", &ch);
    
        if ( ch == 'y')
        {
            
        }
        
        else if ( ch == 'n')
        {
            printf("Program ended\n");
            break;
        }
    }
}
