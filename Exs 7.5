#include <stdio.h>

void main()
{
    int marks;
    
    printf("Enter marks : ");
    scanf("%d", &marks);
    
    if (marks >= 75)
        printf("%d is Grade A\n", marks);
    
    else if ( 60 <= marks && marks < 75)
    
        printf("%d is grade B\n", marks);
    
    else if ( 45 <= marks && marks < 60)
    
        printf("%d is grade C\n", marks);
    
    else if ( 35 <= marks && marks <45 )
    
        printf("%d is grade D\n", marks);
    
    else
        printf("%d is grade E\n", marks);
    }
