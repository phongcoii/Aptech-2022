#include <stdio.h>

int main ()
{
    int num1, num2;
    num1 = 77;
    num2 = 90;
    
    if ( num1 == num2 )
        printf("\n The numbers are equal\n");
    else if (num1 < num2)
        printf(" The greater number is %d\n", num2);
    else
        printf(" The greater number is %d\n", num1);
    
    return 0;
}
